{
  "expo": {
    "name": "red-protocol",
    "slug": "red-protocol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.chukwudavid.redprotocol"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "redprotocol",
    "plugins": [
      "expo-web-browser"
    ]
  }
}
